    "formik": "^2.2.5",
    "gatsby": "^2.26.1",
    "gatsby-plugin-apollo": "^3.0.2",
    "gatsby-plugin-typescript": "^2.8.0",
    "nanoid": "^3.1.20",
    "rc-color-picker": "^1.2.6",
    "react": "^16.12.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.1.2",
    "yup": "^0.32.5"
  },
  "devDependencies": {
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
